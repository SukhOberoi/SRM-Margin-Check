{
  "manifest_version": 3,
  "name": "SRM Academia Extended",
  "version": "1.0",
  "description": "Makes academia more user-friendly.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://sp.srmist.edu.in/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://academia.srmist.edu.in/*"],
      "js": ["content1.js"]
    }
  ],
  "action": {
      "default_popup": "popup.html"
  }
}
